   "cell_type": "code",
   "execution_count": null,
   "id": "5856557c",
   "metadata": {},
   "outputs": [],
   "source": [
    "lastname = input('Enter student last name: ')\n",
    "while lastname != \"ZZZ\":\n",
    "    firstname = input('Enter student first name: ')\n",
    "    gpa = float(input(\"Enter student GPA: \"))\n",
    "    \n",
    "    if gpa >= 3.5:\n",
    "        print(\"{} {} has made it to the Dean's List\".format(firstname, lastname))\n",
    "    else:\n",
    "        if gpa >= 3.25:\n",
    "            print(\"{} {} has made it to the Honor Roll\".format(firstname, lastname))\n",
    "    lastname = input(\"\\nEnter Student Last Name: \")"
   ]
